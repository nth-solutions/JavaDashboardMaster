            <Tab fx:id="SINCCalibrationHelpTab" text="SINCCalibrationHelpTab">
               <content>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <VBox layoutY="26.0" prefHeight="599.0" prefWidth="1001.0" AnchorPane.topAnchor="26.0">
