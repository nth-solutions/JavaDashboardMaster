                              <Insets left="100.0" />
                           </opaqueInsets>
                        </Label>
                        <Label contentDisplay="CENTER" layoutX="50.0" layoutY="378.0" prefHeight="21.0" prefWidth="933.0" text="3. Test Paired Remotes" textAlignment="JUSTIFY" wrapText="true">
                           <font>
